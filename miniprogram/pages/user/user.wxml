<view class="page" data-weui-theme="{{theme}}" style="background:white">
	
	<!-- 个人中心 -->
  <view class="weui-cells weui-cells_after-title user-cell-body page_bd user-font">
      <!-- <view >
        <view class="weui-cells__title" style="margin-top:90rpx; text-align:center;margin-bottom:60rpx;">
          <image style="width:150rpx;height:150rpx;" src="../../img/logo.png"></image>
          <view style="font-size:1em;">Mebs SmartHome v1.0.1</view>
        </view>
      </view> -->
      <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" style="height:80px">
          <view class="weui-cell__hd">
              <image src="{{avatarUrl}}" style="margin-right: 16px;vertical-align: middle;width:50px; height: 50px;margin-top:-10rpx;"></image>
          </view>
          <view class="weui-cell__bd">
            <view>欢迎您，{{userInfo.nickName}}</view>
            <view style="font-size: 13px; color: #888;">{{phone}}</view>
        </view>
          <view class="weui-cell__ft weui-cell__ft_in-access "></view>
      </navigator>

      <view style="background-color:#EDEDED; width:100%;height:30rpx; "></view>
      <view class="weui-cells weui-cells_after-title user-font" style="margin-top:-10rpx;">
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active"  bindtap='basicSettings'>
              <view class="weui-cell__bd">基础设置</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" catchtap="toFloor" >
              <view class="weui-cell__bd" >楼层设置</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" catchtap="toRoom">
              <view class="weui-cell__bd">房间设置</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" catchtap="toWIFI">
              <view class="weui-cell__bd">WIFI设置</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
      </view>

      <view style="background-color:#EDEDED; width:100%;height:30rpx;"></view>
      <view class="weui-cells weui-cells_after-title user-font" style="margin-top:-10rpx;">
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd">添加设备</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd">绑定设备</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
        
      </view>
      <view style="background-color:#EDEDED; width:100%;height:30rpx;"></view>
      <view class="weui-cells weui-cells_after-title user-font" style="margin-top:-10rpx;">
          <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
              <view class="weui-cell__bd">订单记录</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>

            <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" catchtap="toNews">
              <view class="weui-cell__bd">消息记录</view>
              <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
      </view>

  </view>

</view>
