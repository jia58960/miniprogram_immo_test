<view class="page" data-weui-theme="{{theme}}" style="background:white">
    
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title"  style="margin-top:-6px; font-size:15px">

            <view class="weui-cell weui-cell_active weui-cell_select weui-cell_select-after internet-font" >
                <view class="weui-cell__hd">
                    <view class="weui-label">小米WiFi</view>
                </view>
                <view class="weui-cell__bd">
                    <picker bindchange="bindCountryChange" value="{{countryIndex}}" range="{{countries}}">
                        <view class="weui-select weui-select_in-select-after">{{countries[countryIndex]}}</view>
                    </picker>
                </view>
            </view>

            <view class="weui-cell weui-cell_active weui-cell_select weui-cell_select-after internet-font">
                <view class="weui-cell__hd">
                    <view class="weui-label">华为wifi名称</view>
                </view>
                <view class="weui-cell__bd">
                    <picker bindchange="bindCountryChange" value="{{countryIndex}}" range="{{countries}}">
                        <view class="weui-select weui-select_in-select-after">{{countries[countryIndex]}}</view>
                    </picker>
                </view>
            </view>

            <view class="weui-cell weui-cell_active weui-cell_select weui-cell_select-after internet-font">
                <view class="weui-cell__hd">
                    <view class="weui-label">360wifi名称</view>
                </view>
                <view class="weui-cell__bd">
                    <picker bindchange="bindCountryChange" value="{{countryIndex}}" range="{{countries}}">
                        <view class="weui-select weui-select_in-select-after">{{countries[countryIndex]}}</view>
                    </picker>
                </view>
        </view>

        </view>

        <view class="button-sp-area addwifi-btn" catchtap="toAddWifi">
            <a class="weui-btn addwifi-btn-text" >添加WIFI</a>
        </view>
      </view>
</view>
